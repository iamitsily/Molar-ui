<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:id="@+id/relativeLayout15"
        android:layout_width="288dp"
        android:layout_height="102dp"
        android:layout_marginBottom="296dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent">


        <View
            android:id="@+id/rectangle_35"
            android:layout_width="273dp"
            android:layout_height="86dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/view_details_cita_paciente_bghora" />

        <View
            android:layout_width="61dp"
            android:layout_height="59dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="27dp"
            android:layout_marginBottom="26dp"
            android:background="@drawable/drawable_patient_reagendar_cita_borderhora" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="26dp"
            android:layout_height="27dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="45dp"
            android:layout_marginTop="33dp"
            app:srcCompat="@mipmap/view_patient_reagendar_citas2_reloj" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="117dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/rectangle_35"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="47dp"
            android:text="Hora de consulta" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="94dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="84dp"
            android:layout_marginBottom="31dp"
            android:text="[Hora]"
            android:textColor="#0E1012"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="38dp"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@mipmap/view_patient_reagendar_citas2_ver" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="288dp"
        android:layout_height="102dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent">


        <View
            android:id="@+id/rectangle_15"
            android:layout_width="273dp"
            android:layout_height="86dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/view_details_cita_paciente_bghora" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="31dp"
            android:layout_height="29dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="38dp"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@mipmap/view_patient_reagendar_citas2_ver" />

        <View
            android:layout_width="61dp"
            android:layout_height="59dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
